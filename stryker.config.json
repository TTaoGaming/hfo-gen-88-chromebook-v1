{
  "$schema": "https://raw.githubusercontent.com/stryker-mutator/stryker-js/master/packages/api/schema/stryker-config.schema.json",
  "packageManager": "npm",
  "reporters": [
    "html",
    "clear-text",
    "progress",
    "json"
  ],
  "testRunner": "command",
  "commandRunner": {
    "command": "npx playwright test -c hfo_hot_obsidian/bronze/2_areas/mission_thread_omega/playwright.config.ts hfo_hot_obsidian/bronze/2_areas/mission_thread_omega/tests/exhaustive_fsm_active.test.ts"
  },
  "coverageAnalysis": "off",
  "ignorePatterns": [
    ".venv/**",
    "node_modules/**",
    "dist/**",
    "test-results/**",
    "hfo_cold_obsidian/**",
    ".stryker-tmp/**",
    ".git/**",
    ".vscode/**",
    "**/*.md",
    "**/*.jsonl",
    "**/rapier/src/**"
  ],
  "mutate": [
    "hfo_hot_obsidian/bronze/2_areas/mission_thread_omega/p3_deliver/gesture_fsm_active.ts"
  ],
  "checkers": [
    "typescript"
  ]
}