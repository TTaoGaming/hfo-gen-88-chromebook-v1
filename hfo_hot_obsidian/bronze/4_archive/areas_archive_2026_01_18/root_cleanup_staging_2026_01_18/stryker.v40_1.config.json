{
  "$schema": "https://stryker-mutator.io/schemas/stryker-config.json",
  "packageManager": "npm",
  "reporters": [
    "html",
    "clear-text",
    "progress"
  ],
  "testRunner": "playwright",
  "playwright": {
    "configFile": "playwright.config.ts"
  },
  "mutate": [
    "hfo_hot_obsidian/bronze/2_areas/mission_thread_omega_gen_4/omega_gen4_v40_1.html"
  ],
  "tempDirName": "stryker-tmp-v40-1",
  "thresholds": {
    "high": 88,
    "low": 80,
    "break": 60
  },
  "concurrency": 2,
  "timeoutMS": 30000,
  "coverageAnalysis": "perTest",
  "ignorer": {
    "type": "comment",
    "pattern": "stryker-ignore"
  }
}