{
  "name": "hfo-gen-88-chromebook",
  "version": "1.0.0",
  "description": "HFO Gen 88 Omega Workspace - Chromebook V-1",
  "scripts": {
    "start": "python3 scripts/hfo_threaded_server.py",
    "start:omega": "python3 scripts/hfo_threaded_server.py & sleep 1 && xdg-open http://localhost:8889/active_hfo_omega_entrypoint.html",
    "launch": "npm run p5:preflight && xdg-open http://localhost:8889/active_hfo_omega_entrypoint.html",
    "hfo:health": "python3 scripts/hfo_healthcheck.py --mode=soft --format=human",
    "hfo:health:strict": "python3 scripts/hfo_healthcheck.py --mode=hard --format=human",
    "p5:preflight": "npm run hfo:health:strict && python3 scripts/p5_preflight_audit.py && npx eslint active_hfo_omega_entrypoint.html active_omega.html",
    "p5:gate": "npx playwright test scripts/p5_health_gate.spec.ts",
    "p4:regression": "npx playwright test tests/p4_visual_regression.spec.ts",
    "test:v35": "npx playwright test tests/v35_layout_integrity.spec.ts",
    "test:parity": "npx playwright test tests/v35_golden_master_parity.spec.ts",
    "harden": "npm run p5:preflight && npm run p5:gate && npm run p4:regression",
    "clean:chrome": "sudo apt purge google-chrome-stable -y && sudo apt autoremove -y",
    "hfo:setup": "npm install && echo 'SKIPPING LINUX-CHROME INSTALL (Use Native Chromebook Browser)'",
    "hfo:probe": "node -r dotenv/config scripts/hfo_host_probe.js",
    "hfo:shot:headless": "node -r dotenv/config scripts/hfo_screenshot.js headless",
    "hfo:shot:host": "node -r dotenv/config scripts/hfo_screenshot.js host",
    "status": "echo 'HFO Stability Status' && netstat -tulpn | grep 8889",
    "test:omega": "npx playwright test",
    "test:omega:cdp": "HFO_CDP_URL=${HFO_CDP_URL:-http://localhost:9222} npx playwright test",
    "test:omega:replay": "npx playwright test scripts/omega_golden_master_replay.spec.ts --project=chromium",
    "test:omega:fsm": "npx playwright test scripts/omega_gen5_video_fsm.spec.ts --project=chromium",
    "test:omega:readiness": "npx playwright test scripts/omega_gen5_readiness_drain.spec.ts --project=chromium",
    "test:omega:energy": "npx playwright test scripts/omega_gen5_readiness_energy.spec.ts --project=chromium",
    "test:omega:excalidraw": "npx playwright test scripts/omega_gen5_excalidraw_replay.spec.ts --project=chromium",
    "test:omega:excalidraw:ready": "npx playwright test scripts/omega_gen5_excalidraw_ready.spec.ts --project=chromium",
    "test:omega:excalidraw:compat": "npx playwright test scripts/omega_gen5_excalidraw_compat.spec.ts --project=chromium",
    "test:omega:emulation:red": "npx playwright test scripts/omega_gen5_pointer_emulation_red.spec.ts --project=chromium",
    "test:omega:injector": "npx playwright test scripts/omega_gen5_injector_harness.spec.ts --project=chromium",
    "lint:omega:gen5": "npx eslint -c ./eslint.config.mjs hfo_hot_obsidian/bronze/1_projects/omega_gen5_current/omega_gen5_v6.html --max-warnings=0",
    "format:omega:gen5": "npx eslint -c ./eslint.config.mjs hfo_hot_obsidian/bronze/1_projects/omega_gen5_current/omega_gen5_v6.html --fix",
    "gate:omega:gen5": "npm run lint:omega:gen5",
    "test:omega:fast:git": "bash scripts/hfo_fast_check.sh",
    "test:omega:fast": "npm run gate:omega:gen5 && npm run test:omega:replay && npm run test:omega:fsm && npm run test:omega:readiness && npm run test:omega:energy && npm run test:omega:excalidraw && npm run test:omega:excalidraw:ready && npm run test:omega:excalidraw:compat && npm run test:omega:injector",
    "test:gen5:v9:smoke": "bash scripts/hfo_gen5_version_smoke.sh omega_gen5_v9.html",
    "test:gen5:v10_1:smoke": "bash scripts/hfo_gen5_version_smoke.sh omega_gen5_v10_1.html",
    "test:gen5:v10_1:smoke:p1ports": "HFO_GEN5_RUN_P1_PORTS_SMOKE=true bash scripts/hfo_gen5_version_smoke.sh omega_gen5_v10_1.html",
    "test:gen5:v10_1:smoke:p7": "HFO_GEN5_RUN_P7_NAV_SMOKE=true bash scripts/hfo_gen5_version_smoke.sh omega_gen5_v10_1.html",
    "test:gen5:v10_2:smoke": "bash scripts/hfo_gen5_version_smoke.sh omega_gen5_v10_2.html",
    "test:gen5:v10_2:smoke:p1ports": "HFO_GEN5_RUN_P1_PORTS_SMOKE=true bash scripts/hfo_gen5_version_smoke.sh omega_gen5_v10_2.html",
    "test:gen5:v10_2:smoke:p7": "HFO_GEN5_RUN_P7_NAV_SMOKE=true bash scripts/hfo_gen5_version_smoke.sh omega_gen5_v10_2.html",
    "test:gen5:v10_2:smoke:p4": "HFO_GEN5_RUN_P4_DISRUPT_SMOKE=true bash scripts/hfo_gen5_version_smoke.sh omega_gen5_v10_2.html",
    "test:gen5:v10_2:clip": "bash scripts/hfo_gen5_clip_replay_check.sh omega_gen5_v10_2.html",
    "test:gen5:v11:smoke": "bash scripts/hfo_gen5_version_smoke.sh omega_gen5_v11.html",
    "test:gen5:v11:smoke:p1ports": "HFO_GEN5_RUN_P1_PORTS_SMOKE=true bash scripts/hfo_gen5_version_smoke.sh omega_gen5_v11.html",
    "test:gen5:v11:smoke:p7": "HFO_GEN5_RUN_P7_NAV_SMOKE=true bash scripts/hfo_gen5_version_smoke.sh omega_gen5_v11.html",
    "test:gen5:v11:smoke:p4": "HFO_GEN5_RUN_P4_DISRUPT_SMOKE=true bash scripts/hfo_gen5_version_smoke.sh omega_gen5_v11.html",
    "test:gen5:v11:clip": "bash scripts/hfo_gen5_clip_replay_check.sh omega_gen5_v11.html",
    "test:gen5:v11:entrypoint": "npx playwright test scripts/omega_gen5_entrypoint_smoke.spec.ts --project=chromium",
    "test:gen5:v11:strict": "npx playwright test scripts/omega_gen5_v11_strict_hex.spec.ts --project=chromium",
    "test:gen5:v11:stigmergy": "npx playwright test scripts/omega_gen5_v11_stigmergy.spec.ts --project=chromium",
    "test:gen5:v11:full": "npm run p5:preflight && bash scripts/hfo_gen5_v11_full_gate.sh omega_gen5_v11.html",
    "test:gen6:v9:dino:classify": "bash scripts/hfo_gen6_v9_dino_classify.sh",
    "precommit:gate": "bash scripts/hfo_precommit_gate.sh",
    "test:omega:e2e": "npm run test:omega",
    "test:fast": "npm run test:omega:fast:git",
    "test:e2e": "npm run test:omega:e2e",
    "cleanup:omega": "bash scripts/omega_gen5_cleanup.sh",
    "prepare": "husky"
  },
  "devDependencies": {
    "@modelcontextprotocol/inspector": "^0.15.0",
    "@modelcontextprotocol/server-filesystem": "^2026.1.14",
    "@modelcontextprotocol/server-memory": "^2025.11.25",
    "@modelcontextprotocol/server-sequential-thinking": "^2025.12.18",
    "@stryker-mutator/core": "^9.4.0",
    "@stryker-mutator/karma-runner": "^9.4.0",
    "@stryker-mutator/typescript-checker": "^9.4.0",
    "dotenv": "^16.6.1",
    "eslint": "^9.39.2",
    "eslint-plugin-html": "^8.1.3",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "mocha": "^11.7.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.25.2",
    "@playwright/test": "^1.57.0",
    "@stabgan/openrouter-mcp-multimodal": "^1.5.0",
    "axios": "^1.13.2",
    "nactor": "^0.2.0",
    "openai": "^6.16.0",
    "zod": "^3.22.4"
  },
  "optionalDependencies": {
    "robotjs": "^0.6.0"
  }
}
